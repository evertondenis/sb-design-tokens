{
  "name": "sb-design-tokens",
  "version": "0.0.1",
  "description": "Storybook addon to extract component tokens",
  "author": "Everton Denis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evertondenis/sb-design-tokens"
  },
  "main": "dist/preset.js",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "keywords": [
